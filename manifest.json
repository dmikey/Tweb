{
  "name": "Tweb",
  "description": "An OpenSource Enyo 2 Twitter Client",
  "version": "1",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "https://*.twitter.com/"
  ]
}
